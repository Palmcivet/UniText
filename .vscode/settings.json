{
  "todo-tree.filtering.excludeGlobs": ["**/*.md", "**/node_modules/**"],
  "todo-tree.general.tags": [
    "FIX", // 待修复 Bug
    "TODO", // 待办
    "FEAT", // 后期添加特性
    "DEV" // 开发时参数
  ],
  "todo-tree.highlights.customHighlight": {
    "FIX": {
      "foreground": "#ff0a0a",
      "iconColour": "#ff0a0a",
      "icon": "alert"
    },
    "TODO": {
      "foreground": "#a2ce3a",
      "iconColour": "#a2ce3a",
      "icon": "check"
    },
    "FEAT": {
      "foreground": "#F9811F",
      "iconColour": "#F9811F",
      "icon": "pencil"
    },
    "NOTE": {
      "foreground": "#F9811F",
      "iconColour": "#F9811F",
      "icon": "bell"
    },
    "DEV": {
      "foreground": "#409dff",
      "iconColour": "#409dff",
      "icon": "git-merge"
    }
  },
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/build": true,
    "**/dist": true,
    "**/*.lock": true
  },
  "files.watcherExclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/build": true,
    "**/dist": true,
    "**/*.lock": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "npm.packageManager": "yarn",
  "npm.exclude": "dist"
}
